{
    "logging": {
        "level": 20
    },

    "nfc": {
        "path": "tty:usbserial-110:pn532",
        "broadcast": true
    },

    "hap": {
        "port": 51926,
        "persist": "hap.state",
        "default": "locked"
    },

    "homekey": {
        "persist": "homekey.json",
        "express": true,
        "finish": "black",
        "flow": "fast",
        "name": "Garage Door Lock",
        "manufacturer": "Unknown Manufacturer",
        "serialNumber": "Unknown Serial Number",
        "model": "Unknown Model",
        "firmware": "1.0"
    }
}
